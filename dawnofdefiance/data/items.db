{"_id":"Hs7Qpq2gnn73C8QD","name":"Ink Cloud (1/Rest)","type":"feat","img":"systems/sw5e/packs/Icons/Species/Quarren.webp","data":{"description":{"value":"<p><span dir=\"ltr\">Each creature in a <strong>10-foot cone</strong> must </span><span dir=\"ltr\">make a <strong>DC 13 Constitution saving throw</strong>. On a failed </span><span dir=\"ltr\">save the creature is <strong>Blinded </strong>until the end of its next </span><span dir=\"ltr\">turn.</span></p>","chat":"","unidentified":""},"requirements":"Quarren","source":"EC","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":null,"units":"inst"},"target":{"value":10,"width":null,"units":"ft","type":"cone"},"range":{"value":null,"long":null,"units":""},"uses":{"value":1,"max":"@attributes.prof","per":"lr"},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"save","attackBonus":0,"chatFlavor":"On a failed save the creature is Blinded until the end of its next turn.","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"con","dc":13,"scaling":"flat"},"recharge":{"value":null,"charged":false},"className":null,"attunement":null},"effects":[{"_id":"90vQir5pwm9bK14g","changes":[{"key":"macro.CUB","mode":0,"value":"Blind","priority":"20"}],"disabled":false,"duration":{"startTime":null},"icon":null,"label":"Blinded","origin":"Item.Hs7Qpq2gnn73C8QD","transfer":false,"flags":{"core":{"statusId":""},"dae":{"stackable":"none","durationExpression":"","specialDuration":["1Action"],"transfer":false}},"tint":null,"selectedKey":"macro.CUB"}],"folder":null,"sort":300000,"permission":{"default":0,"vorSwUmgv9YMdSPD":3,"7C4n5U6t3s1t1j3A":3},"flags":{"timestamp":"2022-03-10T09:26:09.3986708+00:00","importer_version":1,"uid":"Feature.name-ink_cloud.source-species.sourceName-quarren.level-None","core":{"sourceId":"Compendium.sw5e.speciesfeatures.QDhVr5KGyYtUOIql"},"midi-qol":{"effectActivation":false},"midiProperties":{"nodam":false,"halfdam":false,"fulldam":false